{
  "name": "formdee",
  "version": "1.3.2",
  "description": "Production-ready dynamic form builder with AI-powered generation, Google Sheets integration, Supabase backend, and comprehensive testing suite",
  "author": "FormDee Development Team",
  "license": "MIT",
  "homepage": "https://github.com/zierocode/FormDee",
  "repository": {
    "type": "git",
    "url": "https://github.com/zierocode/FormDee.git"
  },
  "bugs": {
    "url": "https://github.com/zierocode/FormDee/issues"
  },
  "keywords": [
    "form-builder",
    "google-sheets",
    "nextjs",
    "typescript",
    "file-upload",
    "google-drive",
    "dynamic-forms",
    "no-database",
    "serverless"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "cross-env NODE_OPTIONS='--max-old-space-size=4096' tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "setup:deployment": "node scripts/setup-deployment.js",
    "setup:supabase": "node scripts/setup-supabase.js",
    "production:check": "node scripts/production-check.js",
    "production:setup": "node scripts/production-setup.js",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "npm run lint && npm run typecheck && npm run build",
    "deploy:vercel": "npm run build:production && vercel --prod",
    "deploy:docker": "docker build -t formdee . && docker run -p 3000:3000 formdee",
    "deploy:docker:auto": "node scripts/docker-deploy.js",
    "deploy:docker:compose": "docker-compose up -d --build",
    "deploy:docker:cleanup": "node scripts/docker-deploy.js --cleanup",
    "docker:logs": "node scripts/docker-manage.js logs",
    "docker:stop": "node scripts/docker-manage.js stop",
    "docker:start": "node scripts/docker-manage.js start",
    "docker:restart": "node scripts/docker-manage.js restart",
    "docker:status": "node scripts/docker-manage.js status",
    "docker:clean": "node scripts/docker-manage.js clean",
    "test": "node tests/test-runner.js help",
    "test:help": "node tests/test-runner.js help",
    "test:install": "npm install --save-dev playwright",
    "// ═══ 4 Main Test Categories ═══": "",
    "test:api:standard": "node tests/test-runner.js api-standard",
    "test:api:full": "node tests/test-runner.js api-full",
    "test:e2e:standard": "npx playwright test tests/e2e/standard-suite.spec.ts",
    "test:e2e:full": "npx playwright test tests/e2e/full-suite.spec.ts",
    "test:e2e:head": "node scripts/smart-e2e-head.js",
    "test:e2e:head:full": "node scripts/smart-e2e-head.js --full",
    "test:e2e:head:verbose": "node scripts/smart-e2e-head.js --verbose",
    "test:e2e:manual:standard": "PLAYWRIGHT_HEAD=true npx playwright test tests/e2e/standard-suite.spec.ts --project=chromium --headed",
    "test:e2e:manual:full": "PLAYWRIGHT_HEAD=true npx playwright test tests/e2e/full-suite.spec.ts --project=chromium --headed",
    "test:e2e:debug": "PLAYWRIGHT_HEAD=true npx playwright test tests/e2e/standard-suite.spec.ts --project=chromium --debug",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:report": "node scripts/e2e-report-manager.js",
    "test:e2e:report:fresh": "node scripts/e2e-report-manager.js fresh",
    "test:e2e:report:info": "node scripts/e2e-report-manager.js info",
    "// ═══ Combined Test Suites ═══": "",
    "test:all:standard": "node tests/test-runner.js all-standard",
    "test:all:full": "node tests/test-runner.js all-full",
    "test:all": "node tests/test-runner.js all",
    "// ═══ Quick Test Commands ═══": "",
    "test:quick": "node tests/test-runner.js quick",
    "test:ci": "node tests/test-runner.js ci",
    "test:cleanup": "node tests/test-runner.js cleanup",
    "test:cleanup:interactive": "node tests/run-cleanup.js --interactive",
    "test:cleanup:universal": "node tests/run-cleanup.js",
    "test:cleanup:api": "node tests/run-cleanup.js --api-only",
    "test:cleanup:e2e": "node tests/run-cleanup.js --e2e-only",
    "test:cleanup:safe": "node tests/run-cleanup.js --interactive",
    "test:safety-check": "node tests/utils/safety-check.js",
    "test:head-mode": "node scripts/test-head-mode.js",
    "// ═══ Legacy/Direct Commands ═══": "",
    "test:api:direct": "node tests/api/api-test-suite.js",
    "test:forms:direct": "./tests/run-tests.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.1",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@aws-sdk/client-s3": "^3.884.0",
    "@aws-sdk/s3-request-presigner": "^3.884.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^3.10.0",
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.3",
    "antd": "^5.27.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "googleapis": "^159.0.0",
    "lucide-react": "^0.542.0",
    "next": "^14.2.32",
    "openai": "^5.20.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "20.11.30",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.19",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^10.4.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.0",
    "postcss": "8.4.38",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^5.0.10",
    "tailwindcss": "3.4.4",
    "tsx": "^4.20.5",
    "typescript": "5.4.5"
  },
  "overrides": {
    "inflight": "npm:@isaacs/inflight@^1.0.6",
    "glob": "^10.4.5",
    "rimraf": "^5.0.10",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@^0.18.0",
    "@humanwhocodes/object-schema": "npm:@eslint/object-schema@^2.1.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
